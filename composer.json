{
    "name": "themadbox/phpday",
    "description": "Phpday2015 base code",
    "homepage": "https://github.com/themadbox/phpday",
    "authors": [
        {
            "name": "Alessandro Gobbo"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/config": "~2.6",
        "symfony/event-dispatcher": "~2.6",
        "symfony/http-kernel": "~2.6",
        "symfony/finder": "~2.6",
        "symfony/Yaml":"~2.6",
        "symfony/routing": "~2.6",
        "symfony/class-loader": "~2.6",
        "symfony/http-foundation": "~2.6",
        "guzzlehttp/guzzle": "^6.0"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6"
    },
    "config": {
      "bin-dir": "bin"
    }
}
